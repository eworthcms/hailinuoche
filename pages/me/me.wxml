<view class='section-bg'>
    <view class='avatar-box'><open-data class="avatar" type="userAvatarUrl"></open-data></view>
    <open-data class="nickName" type="userNickName"></open-data>
</view>
<view class='section-img'><image class='icon' mode='widthFix' src='../images/bg3.png'></image></view>

<view class='section-1'>
    <view class='cont'>
        <view class='item' catchtap='linkOrderlist'>
            <image class='icon' mode='widthFix' src='../images/icon-order.png'></image>
            <text class='text'>我的订单</text>
        </view>
        <view class='item' catchtap='linkCodelist'>
            <image class='icon' mode='widthFix' src='../images/icon-nuochema.png'></image>
            <text class='text'>我的挪车码</text>
        </view>
    </view>
</view>

<view class='cont-list'>
    <view class='list' catchtap='linkProblemList'>
        <view class='col-1'><image mode='widthFix' src='../images/icon-wenti.png'></image></view>
        <view class='col-2'>常见问题</view>
        <view class='col-3'><image mode='widthFix' src='../images/icon-arrow1.png'></image></view>
    </view>
    <view class='list' catchtap='contactKefu'>
        <view class='col-1'><image mode='widthFix' src='../images/icon-kefu.png'></image></view>
        <view class='col-2'>联系客服</view>
        <view class='col-3'><image mode='widthFix' src='../images/icon-arrow1.png'></image></view>
    </view>
</view>