page{ padding: 20rpx 20rpx 10rpx; box-sizing: border-box; }

.problemt-list{ background-color: #fff; margin-bottom: 20rpx; border-radius: 10rpx; }
.problemt-list .p-header{ color: #333; font-size: 28rpx; padding: 20rpx 30rpx; position: relative; }
.problemt-list .p-header::after{ content: ""; display: block; position: absolute; left: 7px; top: 11px; width: 8rpx; height: 30rpx; background-color: #1075c1; }
.problemt-list .p-bodyer{ padding: 10rpx 30rpx 25rpx; font-size: 24rpx; color: #333; }