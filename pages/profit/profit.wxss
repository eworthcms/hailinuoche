.section-bg{ height: 400rpx; background-color: #fff; text-align: center; margin-top: 30rpx; } 

.item{ text-align: center; }
.item text{ display: block; }
.item .row1{ color: #111; font-size: 30rpx; padding: 40rpx 0 20rpx; }
.item .row2{ color: #f14a5e; font-size: 64rpx; font-family:PingFangSC-Medium; font-weight: bold; }

.btns{ padding: 40rpx 40rpx 0; }
