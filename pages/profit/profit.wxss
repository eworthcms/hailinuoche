.btns .btn{ height: 90rpx; line-height: 90rpx; background-color: #ddd; color: #fff; border-radius: 200rpx; font-size: 30rpx; text-align: center; box-shadow:0 4px 4px 0 rgba(153,153,153,0.50); }

.profit-list{ background-color: #fff; margin-top: 30rpx; }
.profit-list .p-title{ color: #999; font-size: 26rpx; padding: 22rpx 30rpx; border-bottom: solid 1rpx #eee;  }
.profit-list .list{ display: flex; align-items: center; padding: 22rpx 30rpx; position: relative; font-size: 28rpx; color: #000; }
.profit-list .list::after{ content: ""; display: block; position: absolute; left: 30rpx; right: 0; bottom: 0; border-top: solid 1rpx #f5f5f5; }
.profit-list .list:nth-last-child(1)::after{ border-top: 0; }
.profit-list .list .col-1{ flex: 1; margin-right: 20rpx; }
.profit-list .list .col-2{ }